//INPUT
#include<stdio.h>
int main(){ 
    int n;
    printf("Enter any number: ");
    scanf("%d", &n);
    for(int i=2;i<=n-1;i++){
    if(n%i==0){
        printf("The given number is composite\n");
        break;
    }
    else{
    printf("The given number is prime\n");
    break;    
    }
    }
    return 0;
}
//OUTPUT
Enter any number: 34
The given number is composite
